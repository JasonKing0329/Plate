<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tv_r32_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="R32"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_r32_dim"
            app:layout_constraintBottom_toBottomOf="@+id/tv_r32_dim"/>

        <TextView
            android:id="@+id/tv_r32_dim"
            style="@style/TvDrawColor"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:text="Dim"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:id="@+id/tv_r32_focus"
            style="@style/TvDrawColor"
            android:layout_marginEnd="16dp"
            android:text="Focus"
            app:layout_constraintEnd_toStartOf="@+id/tv_r32_dim"
            app:layout_constraintTop_toTopOf="@+id/tv_r32_dim" />

        <TextView
            android:id="@+id/tv_r16_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="R16"
            app:layout_constraintStart_toStartOf="@id/tv_r32_title"
            app:layout_constraintTop_toTopOf="@+id/tv_r16_dim"
            app:layout_constraintBottom_toBottomOf="@+id/tv_r16_dim"/>

        <TextView
            android:id="@+id/tv_r16_dim"
            style="@style/TvDrawColor"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:text="Dim"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_r32_dim" />
        <TextView
            android:id="@+id/tv_r16_focus"
            style="@style/TvDrawColor"
            android:layout_marginEnd="16dp"
            android:text="Focus"
            app:layout_constraintEnd_toStartOf="@+id/tv_r16_dim"
            app:layout_constraintTop_toTopOf="@+id/tv_r16_dim" />

        <TextView
            android:id="@+id/tv_qf_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="QF"
            app:layout_constraintStart_toStartOf="@id/tv_r32_title"
            app:layout_constraintTop_toTopOf="@+id/tv_qf_dim"
            app:layout_constraintBottom_toBottomOf="@+id/tv_qf_dim"/>

        <TextView
            android:id="@+id/tv_qf_dim"
            style="@style/TvDrawColor"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:text="Dim"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_r16_dim" />
        <TextView
            android:id="@+id/tv_qf_focus"
            style="@style/TvDrawColor"
            android:layout_marginEnd="16dp"
            android:text="Focus"
            app:layout_constraintEnd_toStartOf="@+id/tv_qf_dim"
            app:layout_constraintTop_toTopOf="@+id/tv_qf_dim" />

        <TextView
            android:id="@+id/tv_sf_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="SF"
            app:layout_constraintStart_toStartOf="@id/tv_r32_title"
            app:layout_constraintTop_toTopOf="@+id/tv_sf_dim"
            app:layout_constraintBottom_toBottomOf="@+id/tv_sf_dim"/>

        <TextView
            android:id="@+id/tv_sf_dim"
            style="@style/TvDrawColor"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:text="Dim"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_qf_dim" />
        <TextView
            android:id="@+id/tv_sf_focus"
            style="@style/TvDrawColor"
            android:layout_marginEnd="16dp"
            android:text="Focus"
            app:layout_constraintEnd_toStartOf="@+id/tv_sf_dim"
            app:layout_constraintTop_toTopOf="@+id/tv_sf_dim" />

        <TextView
            android:id="@+id/tv_f_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="F"
            app:layout_constraintStart_toStartOf="@id/tv_r32_title"
            app:layout_constraintTop_toTopOf="@+id/tv_f"
            app:layout_constraintBottom_toBottomOf="@+id/tv_f"/>
        <TextView
            android:id="@+id/tv_f"
            style="@style/TvDrawColor"
            android:layout_marginTop="8dp"
            android:text=""
            app:layout_constraintEnd_toEndOf="@+id/tv_sf_focus"
            app:layout_constraintTop_toBottomOf="@+id/tv_sf_dim" />

        <TextView
            android:id="@+id/tv_w_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Winner"
            app:layout_constraintStart_toStartOf="@id/tv_r32_title"
            app:layout_constraintTop_toTopOf="@+id/tv_w"
            app:layout_constraintBottom_toBottomOf="@+id/tv_w"/>
        <TextView
            android:id="@+id/tv_w"
            style="@style/TvDrawColor"
            android:layout_marginTop="8dp"
            android:text=""
            app:layout_constraintEnd_toEndOf="@+id/tv_f"
            app:layout_constraintTop_toBottomOf="@+id/tv_f" />

        <TextView
            android:id="@+id/tv_ok"
            style="@style/TvDialogOk"
            android:layout_marginTop="16dp"
            android:text="Ok"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_w" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>